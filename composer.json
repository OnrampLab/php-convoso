{
    "name": "onramplab/convoso",
    "require": {
        "php": "^7.1.3|^8.0",
        "guzzlehttp/guzzle": "^6.3",
        "illuminate/support": "^8.0",
        "onramplab/onr-phpcs-laravel": "^1.1",
        "squizlabs/php_codesniffer": "*"
    },
    "autoload": {
        "psr-4": {
            "OnrampLab\\Convoso\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.0-dev"
        },
        "laravel": {
            "providers": [
                "OnrampLab\\Convoso\\ConvosoServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    },
    "prefer-stable": true,
    "minimum-stability": "dev",
    "require-dev": {
        "phpstan/phpstan": "^1.7",
        "phpunit/phpunit": "^8.0",
        "sempro/phpunit-pretty-print": "^1.4"
    }
}
